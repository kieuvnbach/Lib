<div id="<%= dom_id member %>">
  <p>
    <strong>Name:</strong>
    <%= member.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= member.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= member.phone %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= member.address %>
  </p>

  <p>
    <strong>Membership date:</strong>
    <%= member.membership_date %>
  </p>

  <p>
    <strong>Membership type:</strong>
    <%= member.membership_type %>
  </p>

</div>
